record ArrowheadSystem {
    SystemName: String,
    Address: String,
    Port: u16,
    AuthenticationInfo: Option<String>,
}

record ArrowheadService {
    ServiceDefinition: String,
    Interfaces: Set<String>,
    ServiceMetadata: Map<String, String>,
}

record ArrowheadCloud {
    Operator: String,
    CloudName: String,
    Address: String,
    Port: u16,
    GatekeeperServiceURI: String,
    AuthenticationInfo: Option<String>,
    IsSecure: Boolean,
}